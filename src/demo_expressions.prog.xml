     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;PURPOSE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Vorstellung der erweiterten Ausrücke in ABAP.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;INTEGRATION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Die Ergebnisse der vorgestellten Ausdrücke werden im Programm berechnet</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>und für die Ausgabe aufbereitet.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;PREREQUISITES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;FEATURES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Es kann aus der Anzeige in den ABAP Debugger verzweigt werden, um die</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Programmierung der Ausdrücke nachzuvollziehen.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U2</TDFORMAT>
       <TDLINE>&amp;SELECTION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Auswahl der Ansicht:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Es kann durch die Präsentation geblättert werden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Es kann ein gesamtes Übersichtsdokument angezeigt werden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Für die Navigation steht in beiden Ansichten ein Inhaltsverzeichnis zur</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Verfügung.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Durch Eingabe einer Eingrezung, wie z.B &quot;SUBSTRING&quot;, kann die</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Präsentation auf bestimmte Funktionen beschränkt werden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U2</TDFORMAT>
       <TDLINE>&amp;STANDARD_VARIANTS&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U2</TDFORMAT>
       <TDLINE>&amp;OUTPUT&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Die erweiterten Ausdrücke werden auf generierten HTML-Seiten</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>präsentiert.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;ACTIVITIES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
