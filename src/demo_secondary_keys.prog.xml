     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>DEMO_SECONDARY_KEYS</TDNAME>
      <TDID>RE</TDID>
      <TDSPRAS>D</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00002</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;PURPOSE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Das Programm demonstriert die Zugriffszeit auf eine interne Tabelle ohne</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>und mit Sekundärschlüssel. Eine dreispaltige interne Tabelle wird einmal</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>über die Angabe eines freien Schlüssels und einmal über die Angabe eines</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>sortierten Sekundärschlüssels nach einem Eintrag durchsucht, der nicht</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>in der Tabelle vorhanden ist. Während bei der Angabe des freien</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Schlüssels alle Tabellenzeilen durchsucht werden müssen, findet bei</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Verwendung des Sekundärschlüssels eine binäre Suche statt.</TDLINE>
      </TLINE>
     </LINES>
